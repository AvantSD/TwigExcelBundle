{
    "name": "recranet/twig-excel-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle provides a PhpExcel integration for Twig",
    "keywords": ["excel","phpexcel","twig","symfony"],
    "homepage": "https://github.com/recranet/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mewes Kochheim",
            "email": "mewes@kochheim.de"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/MewesK/TwigExcelBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": ">=3.0",
        "phpoffice/phpexcel": "~1.8.1"
    },
    "require-dev": {
        "symfony/symfony": ">=3.0",
        "symfony/phpunit-bridge": ">=3.0",
        "sensio/framework-extra-bundle": ">=3.0"
    },
    "autoload": {
        "psr-4": { "Recranet\\TwigExcelBundle\\": "" }
    }
